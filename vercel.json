{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/annotation/browser"
      }
    }
  ],
 "routes": [
  { "src": "/favicon.ico", "dest": "/favicon.ico" },
  { "src": "/(assets/.+)", "dest": "/$1" },
  { "src": "/(.*)", "dest": "/index.html" }
]
}